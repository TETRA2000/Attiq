<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="MultiSwipeRefreshLayout">
    <attr format="reference" name="foreground"/>
  </declare-styleable>

  <declare-styleable name="FloatingActionButton">
    <attr name="backgroundTint"/>
    <attr name="backgroundTintMode"/>
    <attr format="color" name="rippleColor"/>
    <attr name="exFabSize"/>
    <attr name="elevation"/>
    <attr format="dimension" name="pressedTranslationZ"/>
    <attr format="dimension" name="borderWidth"/>
  </declare-styleable>

  <attr name="exFabSize">
    <enum name="normal" value="0"/>
    <enum name="mini" value="1"/>
    <enum name="big" value="2"/>
  </attr>

  <declare-styleable name="NotBadImageButton">
    <attr name="elevation"/>
  </declare-styleable>
</resources>