<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="MultiSwipeRefreshLayout">
    <attr format="reference" name="foreground"/>
  </declare-styleable>

  <declare-styleable name="FloatingActionButton">
    <attr name="backgroundTint"/>
    <attr name="backgroundTintMode"/>
    <attr format="color" name="rippleColor"/>
    <attr name="exFabSize"/>
    <attr name="elevation"/>
    <attr format="dimension" name="pressedTranslationZ"/>
    <attr format="dimension" name="borderWidth"/>
  </declare-styleable>

  <attr name="exFabSize">
    <enum name="normal" value="0"/>
    <enum name="mini" value="1"/>
    <enum name="big" value="2"/>
  </attr>

  <declare-styleable name="NotBadImageButton">
    <attr name="elevation"/>
  </declare-styleable>

  <attr format="dimension" name="lineHeightHint"/>
  <attr format="dimension" name="topPaddingHint"/>

  <attr format="string" name="font"/>

  <declare-styleable name="FontTextAppearance">
    <attr name="android:textSize"/>
    <attr name="android:textColor"/>
    <attr name="android:fontFamily"/>
    <attr name="font"/>
  </declare-styleable>

  <declare-styleable name="BaselineGridTextView">
    <attr format="float" name="lineHeightMultiplierHint"/>
    <attr name="lineHeightHint"/>
    <attr name="topPaddingHint"/>
  </declare-styleable>

  <declare-styleable name="FontTextView">
    <attr name="font"/>
    <attr name="android:textAppearance"/>
  </declare-styleable>
</resources>